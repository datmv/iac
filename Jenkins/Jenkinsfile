pipeline{
    agent any
    parameters {
        choice choices: ['dat-1', 'dat-2'], name: 'Name'
    }
    stages{
        stage("choose name"){
            if(${params.name} == 'dat-1'){
                echo 'Helle dat-1'
            }   else {
                sh "echo 'Hello dat-2!'"
            }
            
        }
    }
}